{
  "name": "test",
  "version": "1.0.0",
  "description": "test description",
  "main": "index.js",
  "scripts": {
    "prepare": "tsc",
    "test": "jest",
    "coveralls": "cat coverage/lcov.info | coveralls"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.{ts,tsx}",
      "**/(?(*.)spec|?(*.)test).{ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**",
      "!**/docs/**"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/__tests__/**/*.{js,jsx,ts,tsx}",
      "!**/(?(*.)spec|?(*.)test).{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**",
      "!**/docs/**"
    ],
    "coverageDirectory": "coverage",
    "coverageProvider": "v8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lupuscaoticus/test.git"
  },
  "keywords": [
    "lupuscaoticus",
    "test",
    "repo"
  ],
  "author": "lupuscaoticus@gmail.com",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/lupuscaoticus/test/issues"
  },
  "homepage": "https://github.com/lupuscaoticus/test#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.3.0",
    "coveralls": "^3.1.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  }
}